//---------------------------------------------------------------------------

#include <stdio.h>
//---------------------------------------------------------------------------

#pragma argsused
int main(int argc, char* argv[])
{
          int sayac = 0, sayi = 0;
          int a = 1, b = 1, c = 1,i;
          printf("Bir Sayi Giriniz : ");
          scanf("%d",&sayi);
          for(i = 0; i < sayi; i++)
          {
              if(sayac != sayi)
              {
                    a=b;
                    b=c;
                    c=a+b;
                    sayac++;
                    printf("%d ",a);
              }
          }
          getchar();
          getchar();
          return 0;
}
//---------------------------------------------------------------------------
